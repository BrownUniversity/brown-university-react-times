{
  "extends": ["brown", "brown/jest", "brown/prettier", "brown/prettier/react"],
  "env": {
    "browser": true
  },
  "rules": {
    "import/no-extraneous-dependencies": "off"
  },
  "settings": {
    "import/resolver": {
      "node": {
        "moduleDirectory": ["node_modules", "./", "jest/utils"]
      }
    }
  }
}
