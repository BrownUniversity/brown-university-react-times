{
  "name": "brown-university-react-times",
  "description": "A time picker modeled after Airbnb's react-dates for use in React-based applications at Brown University",
  "author": "Colin Cummings <colin_cummings@brown.edu>",
  "version": "0.1.6",
  "private": true,
  "homepage": "https://bitbucket.brown.edu/projects/react/repos/brown-university-react-times",
  "main": "dist/index.commonjs.js",
  "module": "dist/es/index.js",
  "sideEffects": "false",
  "scripts": {
    "lint": "eslint src stories",
    "prettier": "prettier \"{src,stories,__{tests,mocks}__}/**/*.+(js|json|css)\"",
    "format:check": "npm run prettier -- --list-different",
    "format:fix": "npm run prettier -- --write",
    "test": "BABEL_ENV=commonjs jest --config ./jest/config.js",
    "test:verbose": "npm test -- --verbose",
    "test:coverage": "npm test -- --coverage",
    "test:coverage:open": "npm run test:coverage && opener ./coverage/lcov-report/index.html",
    "test:watch": "npm test -- --watch --verbose false",
    "validate": "npm-run-all lint format:check test:coverage",
    "build:commonjs": "BABEL_ENV=commonjs webpack --config webpack/config.js",
    "build:commonjs:analyze": "RUN_WEBPACK_BUNDLE_ANALYZER=true npm run build:commonjs",
    "prebuild:es": "rimraf dist/es",
    "build:es": "BABEL_ENV=es babel src -d dist/es",
    "postbuild:es": "rimraf dist/es/components/__tests__ dist/es/components/utils",
    "prebuild": "npm run validate",
    "build": "npm-run-all build:commonjs build:es",
    "start": "start-storybook -p 8080 -c .storybook"
  },
  "peerDependencies": {
    "brown-university-styles": "git+https://bitbucket.brown.edu/scm/react/brown-university-styles.git#semver:^0.3",
    "react": "^16.0.0",
    "styled-components": "^4.0.0"
  },
  "optionalDependencies": {
    "react-testing-library": "^6.0.0"
  },
  "dependencies": {
    "prop-types": "^15.6.2",
    "react-fns": "^1.4.0",
    "react-popper": "^1.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-knobs": "^5.0.5",
    "@storybook/react": "^5.0.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-inline-react-svg": "^1.0.1",
    "babel-plugin-styled-components": "^1.9.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.21",
    "brown-university-styles": "git+https://bitbucket.brown.edu/scm/react/brown-university-styles.git#semver:^0.3",
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "^1.0.1",
    "eslint": "^5.9.0",
    "eslint-config-brown": "git+https://bitbucket.brown.edu/scm/node/eslint-config-brown.git#semver:^0.12",
    "file-loader": "^2.0.0",
    "formik": "^1.5.1",
    "jest": "^24.1.0",
    "jest-dom": "^3.1.2",
    "jest-styled-components": "^6.3.1",
    "npm-run-all": "^4.1.3",
    "opener": "^1.5.1",
    "prettier": "^1.15.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-svg-loader": "^2.1.0",
    "react-testing-library": "^6.0.0",
    "regenerator-runtime": "^0.13.1",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "styled-components": "^4.1.3",
    "webpack": "^4.26.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "yup": "^0.27.0"
  }
}
