{
  "name": "brown-university-react-times",
  "description": "A time picker modeled after Airbnb's react-dates for use in React-based applications at Brown University",
  "author": "Colin Cummings <colin_cummings@brown.edu>",
  "version": "1.1.0",
  "private": true,
  "homepage": "https://github.com/BrownUniversity/brown-university-react-times",
  "main": "dist/index.commonjs.js",
  "module": "dist/es/index.js",
  "sideEffects": false,
  "scripts": {
    "lint": "eslint src stories",
    "prettier": "prettier '**/*.{js,json}'",
    "format:check": "npm run prettier -- --list-different",
    "format:fix": "npm run prettier -- --write",
    "test": "jest --config ./jest/config.js",
    "test:verbose": "npm test -- --verbose",
    "test:coverage": "npm test -- --coverage",
    "test:coverage:open": "npm run test:coverage && opener ./coverage/lcov-report/index.html",
    "test:watch": "npm test -- --watch --verbose false",
    "validate": "npm-run-all lint format:check test:coverage",
    "build:commonjs": "webpack --config webpack.config.js",
    "build:commonjs:analyze": "RUN_WEBPACK_BUNDLE_ANALYZER=true npm run build:commonjs",
    "prebuild:docs": "rimraf docs",
    "build:docs": "storybook build -o docs",
    "prebuild:es": "rimraf dist/es",
    "build:es": "BABEL_ENV=es babel src -d dist/es",
    "postbuild:es": "rimraf dist/es/components/__tests__ dist/es/components/utils/TimePickerContainer.js",
    "prebuild": "npm run validate",
    "build": "npm-run-all build:commonjs build:es",
    "start": "storybook dev -p 8080 -c .storybook"
  },
  "peerDependencies": {
    "brown-university-styles": "github:BrownUniversity/brown-university-styles#semver:^0.12",
    "react": "^16.0.0",
    "styled-components": "^4.0.0"
  },
  "optionalDependencies": {
    "@testing-library/react": "^8.0.0"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "react-fns": "^1.4.0",
    "react-popper": "^1.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-a11y": "^7.6.17",
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-knobs": "^7.0.2",
    "@storybook/addons": "^7.6.17",
    "@storybook/react": "^7.6.17",
    "@storybook/react-webpack5": "^7.6.17",
    "@testing-library/react": "^8.0.1",
    "babel-jest": "^29.7.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-inline-react-svg": "^1.1.0",
    "babel-plugin-styled-components": "^2.1.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.21",
    "brown-university-styles": "github:BrownUniversity/brown-university-styles#semver:^0.12",
    "eslint": "^8.57.0",
    "eslint-config-brown": "github:BrownUniversity/eslint-config-brown#semver:^0.19.0",
    "formik": "^2.4.5",
    "jest": "^29.7.0",
    "jest-dom": "^3.5.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-styled-components": "^6.3.1",
    "npm-run-all": "^4.1.3",
    "opener": "^1.5.1",
    "prettier": "^3.2.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-svg-loader": "^3.0.3",
    "regenerator-runtime": "^0.13.1",
    "rimraf": "^2.6.3",
    "storybook": "^7.6.17",
    "styled-components": "^4.3.1",
    "webpack": "^5.90.3",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "yup": "^0.27.0"
  }
}
